<RelativeLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	xmlns:tools="http://schemas.android.com/tools"
	android:id="@+id/first_page_constraint_layout"
	android:layout_width="match_parent"
	android:layout_height="match_parent"
	android:background="@color/first_page_activity_first_page_constraint_layout_background_color">



	<android.support.v7.widget.Toolbar
		android:id="@+id/toolbar"
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:background="@color/first_page_activity_toolbar_background_color"
		/>
	<android.support.v7.widget.RecyclerView
		android:layout_below="@id/toolbar"
		android:id="@+id/warranties_section_recycler_view"
		android:layout_width="match_parent"
		android:layout_height="@dimen/first_page_activity_warranties_section_recycler_view_height"
		android:layout_marginTop="@dimen/first_page_activity_warranties_section_recycler_view_margin_top"
		android:background="@drawable/first_page_activity_warranties_section_recycler_view_background"
		tools:listitem="@layout/warranty1_view_holder" />
</RelativeLayout>